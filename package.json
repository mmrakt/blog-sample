{
  "name": "gatsby-starter-netlify-cms",
  "description": "Example Gatsby, and Netlify CMS project",
  "version": "1.1.3",
  "author": "Austin Green",
  "scripts": {
    "clean": "gatsby clean",
    "dev": "ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop",
    "start": "npm run develop",
    "build": "npm run clean && gatsby build",
    "develop": "npm run clean && gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.ts,.jsx,.tsx ./src",
    "prettier:watch": "onchange '**/*.{ts,tsx,js}' -- prettier --config ./.prettierrc --write {{changed}}"
  },
  "dependencies": {
    "@headlessui/react": "^1.0.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "autoprefixer": "^10.2.5",
    "bulma": "^0.9.0",
    "classnames": "^2.3.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.20.35",
    "gatsby-image": "^3.2.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-image": "^1.2.1",
    "gatsby-plugin-postcss": "^4.2.0",
    "gatsby-plugin-prettier-eslint": "^1.0.4",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^2.2.4",
    "gatsby-plugin-sharp": "^2.5.7",
    "gatsby-plugin-typescript": "^3.2.0",
    "gatsby-remark-copy-linked-files": "^2.2.4",
    "gatsby-remark-images": "^3.2.6",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.2.5",
    "gatsby-source-graphcms": "^2.5.0-next.3",
    "gatsby-source-graphql": "^3.2.0",
    "gatsby-transformer-remark": "^2.7.5",
    "gatsby-transformer-sharp": "^2.4.7",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.4",
    "material-ui-popup-state": "^1.8.2",
    "node-sass": "^4.14.0",
    "onchange": "^7.1.0",
    "postcss": "^8.2.9",
    "prop-types": "^15.6.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^6.0.0",
    "react-responsive": "^8.2.0",
    "tailwindcss": "^2.0.4",
    "typescript": "^4.2.3",
    "uuid": "^7.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.3",
    "gatsby-plugin-eslint": "^3.0.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
